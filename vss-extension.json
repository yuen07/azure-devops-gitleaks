{
    "manifestVersion": 1,
    "id": "GitleaksAdvOpt",
    "name": "Gitleaks Advance Options",
    "publisher": "yching007",
    "description": "This extension provides scanning your repo's for leaks with gitleaks. Alternative to Credscan. This is a fork of the project from JoostVoskuil/azure-devops-gitleaks and gives further options",
    "version": "0.0.2",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "icons": {
        "default": "images/logo.png"
    },
    "links": {
        "support": {
            "uri": "https://github.com/yuen07/azure-devops-gitleaks"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/yuen07/azure-devops-gitleaks"
    },
    "categories": [
        "Azure Pipelines"
    ],
    "files": [
        {
            "path": "task"
        }
    ],
    "contributions": [
        {
            "id": "yching007-gitleaks",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task"
            }
        }
    ]
}
